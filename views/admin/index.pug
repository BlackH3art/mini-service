extends ../layout

block content
  h1= title

  nav
    ul
      li
        a(href='/admin/news/add') Add news

  table
    tr
      th id
      th title
      th add date
      th action
    each article, index in data
      tr
        td=index
        td=article.title
        td=article.created
        td
          a(href=`/admin/news/delete/${article._id}`) delete

